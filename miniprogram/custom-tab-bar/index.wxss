/* custom-tab-bar/index.wxss */
.custom-tabs {
  display: flex;
  padding: 25rpx 0;
  background: white;
  box-shadow: 0px 0px 18rpx 2rpx rgba(0, 0, 0, 0.1);
  position: relative;
}

.custom-tab {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  transition: color 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
}

.custom-tab.is-active{
  color:rgb(84, 159, 223);
}